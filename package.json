{
  "name": "react-demo",
  "version": "0.0.0",
  "description": "skeleton react project with ssr",
  "keywords": ["react", "ssr", "webpack", "node", "express"],
  "scripts": {
    "webpack": "webpack --env.target=${WEBPACK_TARGET} --mode=${WEBPACK_MODE} --progress",
    "webpack:web": "WEBPACK_TARGET=web WEBPACK_MODE=${NODE_ENV} npm run webpack",
    "webpack:node": "WEBPACK_TARGET=node WEBPACK_MODE=${NODE_ENV} npm run webpack",
    "webpack:all": "echo \"webpack:web webpack:node\" | xargs -t -P2 -n1 npm run",
    "webpack:web:development": "NODE_ENV=development npm run webpack:web",
    "webpack:node:development": "NODE_ENV=development npm run webpack:node",
    "webpack:web:development:watch": "webpack --config webpack.config.web.js --env.target=web --mode=development --watch",
    "webpack:node:development:watch": "webpack --config webpack.config.node.js --env.target=node --mode=development --watch",
    "build": "WEBPACK_MODE=${NODE_ENV} npm run webpack:all",
    "start": "node index.js",
    "start:nodemon": "nodemon index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "assets-webpack-plugin": "^3.9.10",
    "config": "^3.2.4",
    "express": "^4.17.3",
    "isomorphic-style-loader": "^5.1.0",
    "node-style-loader": "^0.0.1-alpha",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-config-wikimedia": "^0.14.1",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.19.1"
  },
  "optionalDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.3.2",
    "file-loader": "^5.0.2",
    "intersection-observer": "^0.7.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-loader": "^2.4.6",
    "style-loader": "^1.0.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
